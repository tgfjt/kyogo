{
  "name": "kyogo",
  "productName": "Kyogo",
  "version": "0.0.0",
  "description": "",
  "main": "main/index.js",
  "scripts": {
    "test": "standard",
    "start": "electron .",
    "dev": "NODE_ENV=development electron . --not-packaged=true --debug=true",
    "bundle": "browserify -e src/app.js -o renderer/bundle.js",
    "watch": "watchify src/app.js -o renderer/bundle.js -dv",
    "build": "electron-packager . --out=dist --asar --overwrite --all"
  },
  "dependencies": {
    "electron": "^1.0.1",
    "electron-debug": "^1.0.0"
  },
  "devDependencies": {
    "browserify": "^13.1.1",
    "choo": "^3.3.0",
    "choo-location-electron": "^2.0.1",
    "choo-persist": "^1.0.3",
    "classnames": "^2.2.5",
    "devtron": "^1.1.0",
    "electron-packager": "^8.0.0",
    "is-url": "^1.2.2",
    "standard": "^8.6.0",
    "watchify": "^3.7.0"
  }
}
